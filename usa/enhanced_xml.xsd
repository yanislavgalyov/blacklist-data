																												<xsd:element name="value" type="xsd:string" />
																											</xsd:sequence>
																											<xsd:attribute name="id" type="xsd:int" use="required" />
																										</xsd:complexType>
																									</xsd:element>
																								</xsd:sequence>
																							</xsd:complexType>
																						</xsd:element>
																					</xsd:sequence>	
