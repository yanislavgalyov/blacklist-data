<?xml version="1.0" encoding="utf-8" ?>
<xsd:schema id="ofacEnhancedXml" targetNamespace="https://sanctionslistservice.ofac.treas.gov/api/PublicationPreview/exports/ENHANCED_XML" elementFormDefault="qualified"
